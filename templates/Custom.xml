    </context>
  </template>
  <template name="timecost" value="long start$VAR$ = System.currentTimeMillis();&#10;    $END$&#10;log.info(&quot;---$VAR$ time cost: &quot; + (System.currentTimeMillis() - start$VAR$) + &quot; ms---&quot;);" description="" toReformat="false" toShortenFQNames="true">
